@{
    ViewData["Title"] = "Activity Log";
}

<div class="container-fluid">
    <div class="row mb-4">
        <div class="col-12">
            <h1 class="h3 mb-0">
                <i class="fas fa-history me-2"></i>@ViewData["Title"]
            </h1>
            <p class="text-muted">Activity log for @ViewBag.AdminName</p>
        </div>
    </div>

    <div class="card card-modern">
        <div class="card-body">
            <div class="alert alert-info">
                Activity log implementation pending
            </div>
        </div>
    </div>
</div>